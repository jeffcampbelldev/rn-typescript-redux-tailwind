{
  "name": "boltplus2",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "ios": "react-native run-ios --scheme 'BoltPlus2' --simulator='iPhone 11'",
    "ios-staging": "react-native run-ios --scheme 'BoltPlus2Staging' --simulator='iPhone 11'",
    "ios-prod": "react-native run-ios --scheme 'BoltPlus2Prod' --simulator='iPhone 11'",
    "android": "react-native run-android --variant=localdebug",
    "android-staging": "react-native run-android --variant=stagingdebug",
    "android-prod": "react-native run-android --variant=proddebug",
    "android:release": "react-native run-android --variant=localrelease",
    "android:release-staging": "react-native run-android --variant=stagingrelease",
    "android:release-prod": "react-native run-android --variant=prodrelease",
    "start": "react-native start",
    "test": "jest",
    "link": "react-native link",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install"
  },
